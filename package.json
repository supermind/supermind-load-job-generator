{
  "name": "supermind-load-job-generator",
  "version": "1.0.0",
  "description": "Generates a large dummy set of Supermind Load Jobs.",
  "main": "source/index.js",
  "license": "private",
  "scripts": {
    "start": "node -r babel-register -r babel-regenerator-runtime source/index.js"
  },
  "babel": {
    "plugins": [
      "transform-flow-strip-types"
    ],
    "presets": [
      "es2015",
      "stage-2",
      "bluebird"
    ]
  },
  "dependencies": {
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-preset-bluebird": "^1.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "^6.17.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babel-register": "^6.16.3",
    "babyparse": "^0.4.6",
    "bluebird": "^3.4.6",
    "lodash": "^4.16.4",
    "moment": "^2.15.1",
    "rimraf": "^2.5.4"
  }
}
